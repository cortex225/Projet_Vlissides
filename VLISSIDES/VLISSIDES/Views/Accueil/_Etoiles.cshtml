@model int
<ul class="list-unstyled list-inline mb-0">
    @for(var compteur =0; compteur< 5;compteur++){
    @if(compteur<Model){
        <li class="list-inline-item me-0" onclic="DonneNote(@compteur+1)">
            <i class="fas fa-star text-warning fa-xs"></i>
        </li>
        }else{
        <li class="list-inline-item me-0 grise">
            <i class="fas fa-star text-warning fa-xs"></i>
        </li>
        }
    }
</ul>